{
  "buildOptions": {
    "allowUnsafe": true,
    "define": [ "UNSAFE" ],
    "compile": [ "*.cs", "../LZ4ps/LZ4Codec.cs" ]
  },
  "configurations": {
    "Debug": {
      "buildOptions": {
        "define": [ "DEBUG" ]
      }
    },
    "Release": {
      "buildOptions": {
        "define": [ "RELEASE" ],
        "optimize": true
      }
    }
  },
  "frameworks": {
    "netstandard1.0": {
      "dependencies": {
        "NETStandard.Library": "1.6.0"
      },
      "buildOptions": {
        "define": [ "NET_CORE", "UNSAFE" ]
      }
    }
  }
}
